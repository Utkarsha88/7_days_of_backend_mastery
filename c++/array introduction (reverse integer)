
#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    
    int A[1000];
    int n;
    cin>>n;
    for (int i =0 ; i<n ;i++){
        cin>>A[i];
        
    }
    for (int j=n-1; j>=0;j--){
        cout<<A[j];
        cout<<" ";
    }
    
    return 0;
}
